            <field name="prifex">HP</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>


        <!-- doctor sequence -->
        <record id="seq_doctor_patient" model="ir.sequence">
            <field name="name">Doctor Sequence</field>
